{
    "pasteImage.path": "${projectRoot}/images",
    "vsnotes.defaultNoteTitle": "{date}-{title}.{ext}",
    "vsnotes.tokens": [
        {
          "type": "datetime",
          "token": "{dt}",
          "format": "YYYY-MM-DD_HH-mm",
          "description": "Insert formatted datetime."
        },
        {
            "type": "datetime",
            "token": "{date}",
            "format": "YYYY-MM-DD",
            "description": "Insert formatted datetime."
          },
        {
          "type": "title",
          "token": "{title}",
          "description": "Insert note title from input box.",
          "format": "Untitled"
        },
        {
          "type": "extension",
          "token": "{ext}",
          "description": "Insert file vsnotes.",
          "format": "md"
        }
      ],
    
}